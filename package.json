{
  "name": "line-diff-details",
  "main": "./lib/line-diff-details",
  "version": "1.9.0",
  "description": "Review/revert individual line changes.",
  "keywords": [
    "diff",
    "line",
    "revert",
    "git"
  ],
  "scripts": {
    "test": "apm test"
  },
  "activationCommands": {},
  "repository": "https://github.com/jakesankey/line-diff-details",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.3",
    "atom-ts-transpiler": "^1.2.3",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "coffeelint": "1.16.0"
  },
  "atomTranspilers": [
        {
            "transpiler": "atom-ts-transpiler",
            "glob": "{!(node_modules)/**/,}*.ts?(x)",
            "options": {
                "cacheKeyFiles": [
                    "lib/tsconfig.json"
                ],
                "verbose": false
            }
        },
        {
            "glob": "{!(node_modules)/**/,}*.js",
            "transpiler": "atom-babel6-transpiler"
        }
    ]
}
